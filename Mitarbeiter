Create Table mitarbeiter
(
mitID int not null,
vorname varchar(20),
nachname varchar(20),
stempelkartennummer int,
statID int,
Primary key (mitID),
FOREIGN KEY (statID) REFERENCES station(statID)
);



mitID, vorname, nachname, stempelkartennummer, statID
1, Andreas, Müller, 200001, 1001
2, Steffen, Kuntz, 200002, 1001
3, Jürgen, Klopp, 200003, 1002
